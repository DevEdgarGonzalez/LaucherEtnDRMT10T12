<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/rlytParentContainerWe"

    android:background="@drawable/fondo_general"
    android:orientation="vertical"
    tools:context="com.actia.nosotros.NosotrosMainActivity">

    <LinearLayout
        android:id="@+id/llytContentDirectoriesWe"
        style="@style/SpaceGnral"
        android:visibility="invisible"
        android:orientation="vertical">
        <LinearLayout style="@style/llytContainerHeaderAndContent"
            android:orientation="horizontal">

            <include
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight=".3"
                layout="@layout/header_category" />


                <FrameLayout
                    android:id="@+id/flytContainerNosotrosMain"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight=".7"
                    android:layout_gravity="center"/>

        </LinearLayout>

    </LinearLayout>

    <FrameLayout
        android:id="@+id/llytContainerParentNosotros"
        style="@style/SpaceGnral"
        android:visibility="invisible" />


</RelativeLayout>