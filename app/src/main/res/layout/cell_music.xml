<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cell_bkg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/item_selector_frid_music_gnr">

    <LinearLayout
        android:id="@+id/bkg_genre"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/grid_item_text_music"
            android:layout_width="fill_parent"
            android:layout_height="88dp"
            android:layout_marginLeft="0dp"
            android:layout_marginRight="0dp"
            android:textColor="@drawable/item_selector_txt_music_children"
            android:textSize="19px"
            android:textStyle="bold"
            android:typeface="sans"></TextView>
    </LinearLayout>
</LinearLayout>
